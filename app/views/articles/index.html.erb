<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <p><%= link_to article.title, article_path(article) %></p>
      <p>By <%= link_to  article.author.username, author_path(article.author) %></p>
    </li>
  <% end %>
</ul>

<%= link_to "Create a New Article", new_article_path, class: "new_article" %>
<%= link_to "Tags", tags_path, class: "new_article"%>

